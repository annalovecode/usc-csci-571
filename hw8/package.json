{
  "name": "usc-csci-571-hw8",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:client": "cd client && ng serve",
    "watch:client": "cd client && ng serve",
    "build:client": "cd client && ng build --prod=true",
    "test:client": "cd client && ng test",
    "lint:client": "cd client && ng lint",
    "e2e:client": "cd client && ng e2e",
    "clean:client": "cd client && rm -rf dist",
    "copy:client": "(mkdir -p server/dist); (cp -r client/dist/client server/dist)",
    "build:server": "cd server && tsc",
    "watch:server": "cd server && tsc-watch --noClear --onSuccess 'node dist/index.js'",
    "start:server": "cd server && tsc && node dist/index.js",
    "clean:server": "cd server && rm -rf dist",
    "build:app": "(npm run build:client); (npm run copy:client); (npm run build:server)",
    "start:app": "(npm run build:client); (npm run copy:client); (npm run start:server)",
    "clean:app": "(npm run clean:client); (npm run clean:server)",
    "build:aws": "(npm run build:client); (npm run copy:client); (zip -r eb-deploy server package.json package-lock.json)",
    "start:aws": "npm run start:server",
    "clean:aws": "(npm run clean:app); (rm -rf eb-deploy.zip)",
    "build:gc": "npm run build:app",
    "start:gc": "cd server && node dist/index.js",
    "clean:gc": "npm run clean:app",
    "build:az": "npm run build:app",
    "start:az": "cd server && node dist/index.js",
    "clean:az": "npm run clean:app",
    "start": "npm run start:app"
  },
